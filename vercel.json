{
  "version": 2,
  "builds": [
    { "src": "api/index.ts", "use": "@vercel/node" },
    { "src": "api/cron/automation.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/cron/automation", "dest": "/api/cron/automation.ts" },
    { "src": "/api/(.*)", "dest": "/api/index.ts" }
  ],
  "crons": [
    { "path": "/api/cron/automation", "schedule": "0 9 * * *" }
  ]
}


